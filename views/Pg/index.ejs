              </div>
              <!-- /Category List -->
              <!-- Category list -->
              <div
                class="col-lg-3 offset-lg-0 col-md-5 offset-md-1 col-sm-6 col-6"
              >
                <div class="category-block">
                  <div class="header">
                    <i
                      class="fa fa-motorcycle icon-bg-5"
                      aria-hidden="true"
                    ></i>
                    <h4>Vehicle Renting</h4>
                  </div>
                </div>
              </div>
              <!-- /Category List -->
              <!-- Category list -->
              <div
                class="col-lg-3 offset-lg-0 col-md-5 offset-md-1 col-sm-6 col-6"
              >
                <div class="category-block">
                  <div class="header">
                    <i class="fa fa-car icon-bg-6"></i>
                    <h4>Transport Facility</h4>
